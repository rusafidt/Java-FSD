
public class test {

	public static void main(String[] args) {
		
		        System.out.println("PATTERN ; ");
		        for(int i=1;i<=5;i++) {
		            for( int j=1;j<i;j++) {
		            System.out.print("1");
		            }
		            if (i==3) {
		            	System.out.println();
		            	System.out.print("11");
		            }
		        System.out.println();
		    }
	}

}
